{
    "name": "jolicode/seo-override",
    "description": "Override your seo markup on the fly",
    "keywords": ["seo"],
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Lo√Øck Piera",
            "email": "pyrech@gmail.com"
        },
        {
            "name": "Damien Alexandre",
            "email": "dalexandre@jolicode.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Joli\\SeoOverride\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Joli\\SeoOverride\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^7.0"
    },
    "require-dev": {
        "ext-sqlite3": "*",
        "doctrine/doctrine-bundle": "~1.6",
        "doctrine/orm": "~2.5",
        "friendsofphp/php-cs-fixer": "~2.2",
        "symfony/config": "~2.7|~3.0",
        "symfony/dependency-injection": "~2.7|~3.0",
        "symfony/framework-bundle": "~2.7.25|~2.8.15|~3.0",
        "symfony/http-kernel": "~2.7|~3.0",
        "symfony/phpunit-bridge": "^3.3",
        "symfony/twig-bundle": "~2.7|~3.0",
        "symfony/yaml": "~2.7|~3.0"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.x-dev"
        }
    },
    "scripts": {
        "test": [
            "SYMFONY_PHPUNIT_REMOVE=\"symfony/yaml\" vendor/bin/simple-phpunit"
        ]
    }
}
